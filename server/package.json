{
  "name": "SUPARAG-server",
  "version": "1.0.0",
  "description": "API Server for SUPARAG - Chat AI with RAG + WhatsApp",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "import": "tsx scripts/run-import.ts",
    "import:example": "tsx scripts/run-import.ts full --path ./data/example --output ./reports",
    "seed:evaluations": "tsx scripts/seed_evaluations.ts generate",
    "seed:clean": "tsx scripts/seed_evaluations.ts clean",
    "seed:audio": "tsx scripts/seed_audio.ts generate",
    "seed:audio:clean": "tsx scripts/seed_audio.ts clean",
    "report:evaluations": "tsx scripts/generate-evaluation-report.ts generate",
    "test:webhook": "tsx scripts/test-whatsapp-webhook.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "rag",
    "ai",
    "whatsapp"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.13.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "sdk-evolution-chatbot": "^0.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.3.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
