{
  "name": "chatbot",
  "bot": {
    "side": "left"
  },
  "user": {
    "side": "right"
  },
  "visual": {
    "theme": "equal",
    "size": {
      "w": "100%",
      "md:max-x": "none",
      "y": "100%",
      "md:max-y": "none"
    },
    "position": "none"
  },
  "behavior": {
    "in": {
      "event": "page.load.full",
      "animation": {
        "start": {
          "opacity": 0,
          "scale": 0.95
        },
        "end": {
          "opacity": 1,
          "scale": 1
        },
        "duration": 300,
        "easing": "easeOut"
      }
    },
    "events": {
      "focus": {
        "scale": 1,
        "opacity": 1,
        "duration": 200
      }
    }
  }
}
